{
  "compilerOptions": {
    "jsx": "react-jsx",
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["src/*"],
      "#root/*": ["./*"]
    }
  },
  "include": ["vite.config.ts"]
}
